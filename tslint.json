{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "impl": true,
        "object-literal-shorthand": false,
        "variable-name": {
            "options": [
                "check-format",
                "require-const-for-all-caps"
            ]
        }
    },
    "rulesDirectory": []
}